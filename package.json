{
  "name": "joi-emoji-constraints",
  "version": "1.0.3",
  "description": "Expect Strings to [not] contain emojis with Joi",
  "main": "index.js",
  "scripts": {
    "test": "istanbul cover tape test.js",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "build:dist": "babel -s true index.js -d dist",
    "serve:dist": "nodemon dist/index.js",
    "clean": "rimraf coverage/ dist/",
    "scan:security": "nsp check",
    "setenv:dev": "npm config set production false",
    "setenv:prod": "npm config set production true",
    "prerelease": "npm run lint && npm run scan:security && npm run test && npm run clean && npm run setenv:prod && npm run build:dist",
    "release": "standard-version",
    "postrelease": "git push --follow-tags origin master && npm publish && npm run setenv:dev"
  },
  "keywords": [
    "emoji",
    "regex",
    "validate",
    "validation",
    "joi",
    "extension"
  ],
  "author": "Jochen Preusche (Iilei)",
  "license": "MIT",
  "peerDependencies": {
    "joi": "^9"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "eslint": "^4.6.1",
    "eslint-config-airbnb-base": "^12.0.0",
    "eslint-plugin-import": "^2.7.0",
    "istanbul": "^0.4.5",
    "joi": "^9.0.4",
    "nsp": "^2.8.0",
    "rimraf": "^2.6.1",
    "standard-version": "^4.2.0",
    "tape": "^4.6.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iilei/joi-emoji-constraints.git"
  },
  "bugs": {
    "url": "https://github.com/iilei/joi-emoji-constraints/issues"
  },
  "homepage": "https://github.com/iilei/joi-emoji-constraints#readme",
  "dependencies": {
    "emoji-regex": "^6.5.1"
  }
}
